<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='realtime'>
  <inherits name='com.goodow.realtime.Realtime'/>
  
  <!-- user.agent=safari is defined by Json.gwt.xml -->
  <extend-property name="user.agent" values="ie8" />
  <extend-property name="user.agent" values="gecko1_8" />
  <extend-property name="user.agent" values="opera" />
  <extend-property name="user.agent" values="ie9" fallback-value="ie8" />
  <collapse-all-properties />
  
  <add-linker name="sso"/>
  <set-configuration-property name="devModeRedirectEnabled" value="false"/>
</module>